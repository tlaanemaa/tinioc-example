{
  "name": "tinioc-example",
  "version": "0.0.0",
  "description": "An example micro-service showcasing tinioc",
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean-dist": "rimraf ./dist",
    "build": "npm run clean-dist && tsc",
    "start": "npm run build && node .",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tlaanemaa/tinioc-example.git"
  },
  "keywords": [
    "tinioc",
    "example"
  ],
  "author": "Taavi Laanemaa <taavi.laanemaa@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tlaanemaa/tinioc-example/issues"
  },
  "homepage": "https://github.com/tlaanemaa/tinioc-example#readme",
  "dependencies": {
    "express": "^4.17.1",
    "node-fetch": "^2.6.2",
    "tinioc": "^0.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node-fetch": "^2.5.12",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "jest": "^27.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  }
}
